unamestr=`uname` 

if [[ "$unamestr" == 'Linux' ]] 
then
  wget -nc -q https://dl.bintray.com/mitchellh/consul/0.4.0_linux_amd64.zip; 
  wget -nc -q https://github.com/jmcarbo/consul_servant/raw/master/bin/consul_servant_linux_amd64
  chmod +x consul_servant_linux_amd64
  mv consul_servant_linux_amd64 consul_servant
  wget -nc -q https://github.com/jmcarbo/consul_servant/raw/master/bin/consul_visa_linux_amd64
  chmod +x consul_visa_linux_amd64
  mv consul_visa_linux_amd64 consul_visa
elif [[ \"$unamestr\" == 'Darwin' ]] 
then 
  wget -nc -q https://dl.bintray.com/mitchellh/consul/0.4.0_darwin_amd64.zip; 
  wget -nc -q https://github.com/jmcarbo/consul_servant/raw/master/bin/consul_servant_darwin_amd64
  chmod +x consul_servant_darwin_amd64
  mv consul_servant_darwin_amd64 consul_servant
  wget -nc -q https://github.com/jmcarbo/consul_servant/raw/master/bin/consul_visa_darwin_amd64
  chmod +x consul_visa_darwin_amd64
  mv consul_visa_darwin_amd64 consul_visa
fi; 

unzip -q 0*
rm 0*

